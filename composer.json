{
    "name": "leovie/configuration",
    "config": {
        "bin-dir": "bin",
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "LeoVie\\Configuration\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LeoVie\\Configuration\\Test\\": "test/"
        }
    },
    "require": {
        "symfony/yaml": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "infection/infection": "^0.13",
        "phpstan/phpstan-shim": "^0.11"
    }
}
